{
    "manifest_version": 2,
    "name": "Coding Train Pop Up",
    "version": "0.1",
    "content_scripts": [
        {
            "matches": [
                "<all_urls>"
            ],
            "js": [
                "jquery.js",
                "content.js"
            ]
        }
    ],
    "permissions": [
        "storage"
    ],
    "commands": {
        "_execute_browser_action": {
            "suggested_key": {
                "windows": "Ctrl+Shift+Y"
            }
        }
    },
    "background": {
        "scripts": [
            "background.js"
        ]
    },
    "browser_action": {
        "default_icon": "icon5.png",
        "default_popup": "popup.html",
        "default_title": "Ctrl+Shift+Y"
    }
}